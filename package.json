{
	"name": "webcontents-api-provider",
	"displayName": "webcontents-api-provider",
	"description": "A minimal Electron template project written with Typescript",
	"version": "0.0.1",
	"main": "dist/index.js",
	"scripts": {
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"lint": "eslint src --ext ts",
		"pretest": "npm run compile && npm run lint",
		"start": "npm run compile && electron ./out/main.js",
		"build:dev": "webpack --config webpack.config.js --env mode=development --env watchChanges=true",
		"pack": "electron-builder --dir",
		"dist": "electron-builder",
		"wait-electron": "wait-on tcp:9223"
	},
	"dependencies": {
		"electron-is-dev": "^2.0.0",
		"uuidv4": "6.0.8",
		"react": "~18.2.0",
		"react-dom": "~18.2.0",
		"rxjs": "~7.8.1",
		"reflect-metadata": "^0.1.13"
	},
	"devDependencies": {
		"@types/react": "~18.2.6",
		"@types/react-dom": "~18.2.4",
		"@types/node": "^16.18.16",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"electron": "^30.0.1",
		"electron-builder": "^22.14.13",
		"eslint": "^8.36.0",
		"typescript": "^5.0.4",
		"wait-on": "^7.0.1",
		"copy-webpack-plugin": "^9.0.1",
		"css-loader": "~6.7.4",
		"html-webpack-plugin": "~5.5.0",
		"ts-loader": "~9.3.0",
		"style-loader": "~3.3.3",
		"terser-webpack-plugin": "~5.3.1",
		"generate-json-webpack-plugin": "^2.0.0",
		"webpack": "~5.64.4",
		"webpack-cli": "~4.9.1",
		"webpack-merge": "~5.8.0",
		"webpack-preprocessor-loader": "~1.3.0"
	},
	"volta": {
		"node": "18.17.1",
		"pnpm": "8.11.0"
	}
}
